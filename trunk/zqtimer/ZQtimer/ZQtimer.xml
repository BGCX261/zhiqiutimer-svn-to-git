                    </Scripts>
                 </StatusBar>
         </Frames>
            <Scripts>
                <OnLoad>
                    self:SetPoint("TOP",self:GetParent(), "BOTTOM", 0 , -ZQTIMER_FRAME_OFFSET_Y)
                    self:Hide()
                </OnLoad>
            </Scripts>
                </Button>
